---
import { Image } from "astro:assets";
import logoImage from "~/assets/logo.png";

// LogoBlock component
export interface Props {
  inClass: string;
  inLoading: string;
}

const { inClass: inClass, inLoading: inLoading } = Astro.props;
---

<Image
  src={logoImage}
  loading={inLoading ? inLoading : "lazy"}
  alt="sealldev"
  class={inClass}
  id="logoblock"
/>
